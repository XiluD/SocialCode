    "ios": {
      "bundleIdentifier": "d.vv.socialcode",
      "buildNumber": "1.0.0",
      "supportsTablet": false
    },
    "android": {
      "package": "d.vv.socialcode",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/app-icon.png"
    }
  }
}
